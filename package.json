{
  "name": "express-boilerplate",
  "version": "0.0.1",
  "description": "Starter project",
  "main": "server/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "sequelize": "sequelize --config server/config/config.json --migrations-path server/migrations --models-path server/models --seeders-path server/seeders",
    "migration:create": "npm run sequelize -- migration:create",
    "migrate": "npm run sequelize -- db:migrate",
    "model:create": "npm run sequelize -- model:create",
    "seed": "npm run sequelize -- db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiamsantos/express-boilerplate.git"
  },
  "author": "Thiago Santos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thiamsantos/express-boilerplate/issues"
  },
  "homepage": "https://github.com/thiamsantos/express-boilerplate#readme",
  "devDependencies": {
    "jest": "^20.0.4",
    "sequelize-cli": "^2.7.0"
  },
  "dependencies": {
    "cpf_cnpj": "^0.2.0",
    "isemail": "^3.0.0",
    "mysql2": "^1.3.6",
    "owasp-password-strength-test": "^1.3.0",
    "secure-password": "^2.1.1",
    "sequelize": "^4.4.2"
  },
  "jest": {
    "automock": false,
    "testEnvironment": "node"
  }
}
